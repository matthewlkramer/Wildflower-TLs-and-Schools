{
  "installCommand": "corepack enable && corepack prepare pnpm@10.14.0 --activate && pnpm install --no-frozen-lockfile",
  "buildCommand": "pnpm -F client build && node -e \"fs=require('fs'); fs.rmSync('dist',{recursive:true,force:true}); fs.cpSync('client/dist','dist',{recursive:true});\"",
  "outputDirectory": "dist",
  "rewrites": [
    { "source": "/((?!api/).*)", "destination": "/index.html" }
  ]
}
